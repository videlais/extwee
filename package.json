{
  "name": "extwee",
  "version": "2.0.0",
  "description": "A Twee 3 compiler written in JS.",
  "author": "Dan Cox",
  "main": "index.js",
  "bin": {
    "extwee": "bin/extwee.js"
  },
  "scripts": {
    "start": "node main.js",
    "test": "jest --coverage --colors",
    "test:watch": "jest --watch",
    "lint-and-test": "npm run lint && npm run lint:test && npm run test",
    "lint": "eslint ./src/**/*.js --fix",
    "lint:test": "eslint ./test/*.test.js --fix"
  },
  "keywords": [
    "twine",
    "twee",
    "parser",
    "compiler"
  ],
  "license": "MIT",
  "dependencies": {
    "minimist": "^1.2.5",
    "node-html-parser": "^1.2.16"
  },
  "devDependencies": {
    "eslint": "^7.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsdoc": "^30.2.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.6.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/videlais/extwee"
  },
  "bugs": {
    "url": "https://github.com/videlais/extwee/issues"
  }
