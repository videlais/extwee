{
  "name": "extwee",
  "version": "2.0.0",
  "description": "A Twee 3 compiler written in JS.",
  "author": "Dan Cox",
  "main": "index.js",
  "bin": {
    "extwee": "bin/extwee.js"
  },
  "scripts": {
    "start": "node main.js",
    "test": "jest --runInBand test/Story.test.js test/Passage.test.js test/StoryFormat.test.js test/FileReader.test.js test/StoryFormatParser.test.js --coverage --colors",
    "test:watch": "jest --watch",
    "lint": "eslint ./src/**/*.js --fix",
    "lint:test": "eslint ./test/*.test.js --fix",
    "all": "npm run lint && npm run lint:test && npm run test"
  },
  "keywords": [
    "twine",
    "twee",
    "parser",
    "compiler"
  ],
  "license": "MIT",
  "dependencies": {
    "minimist": "^1.2.5",
    "node-html-parser": "^1.2.16"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/eslint-plugin": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "babel-loader": "^8.2.2",
    "core-js": "^3.8.1",
    "eslint": "^7.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsdoc": "^30.2.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.6.3",
    "semver": "^5.7.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/videlais/extwee"
  },
  "bugs": {
    "url": "https://github.com/videlais/extwee/issues"
  },
  "type": "module"
}
