{
  "name": "extwee",
  "version": "2.2.0",
  "description": "A story and Twee compiler tool using Twine-compatible formats",
  "author": "Dan Cox",
  "main": "index.js",
  "bin": {
    "extwee": "bin/extwee.js"
  },
  "scripts": {
    "test": "jest --silent --runInBand --coverage --colors",
    "lint": "eslint ./src/**/*.js --fix",
    "lint:test": "eslint ./test/*.test.js --fix",
    "docs": "jsdoc --configure jsdocs.json --verbose",
    "all": "npm run lint && npm run lint:test && npm run test && npm run docs"
  },
  "keywords": [
    "twine",
    "twee",
    "parser",
    "compiler"
  ],
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0",
    "node-html-parser": "^6.1.10",
    "pickleparser": "^0.1.0",
    "semver": "^7.3.7",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/core": "^7.22.20",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/eslint-plugin": "^7.22.10",
    "@babel/preset-env": "^7.22.20",
    "babel-loader": "^9.1.3",
    "clean-jsdoc-theme": "^4.2.10",
    "core-js": "^3.32.2",
    "eslint": "^8.49.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.0",
    "eslint-plugin-jsdoc": "^46.8.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "regenerator-runtime": "^0.14.0",
    "shelljs": "^0.8.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/videlais/extwee"
  },
  "bugs": {
    "url": "https://github.com/videlais/extwee/issues"
  },
  "type": "module"
}
